%div
  %br/

  .row
    .col-md-12.col-sm-12.colxs-12
      #destination_wise_chart  
  .row
    .col-md-8.col-sm-8.colxs-8
      #agent_wise_report
    .col-md-4.col-sm-4.col-xs-4
      #destination_wise_pie_chart

  .row
    .col-md-12.col-sm-12.col-xs-12
      .dashboard_graph.x_panel
        .row.x_title
          .col-md-6
            %h3
              Tickets Sold
              %small Graph title sub-title
          .col-md-6
            #reportrange.pull-right{:style => "background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc"}
              %i.glyphicon.glyphicon-calendar.fa.fa-calendar
              %span December 30, 2014 - January 28, 2015
              %b.caret
        .x_content
          .demo-container{:style => "height:250px"}
            #placeholder3xx3.demo-placeholder{:style => "width: 100%; height:250px;"}

  .row
    .col-md-4.col-sm-6.col-xs-12
      .x_panel.fixed_height_320
        .x_title
          %h2
            Profile Settings
            %small Sessions
          %ul.nav.navbar-right.panel_toolbox
            %li
              %a.collapse-link
                %i.fa.fa-chevron-up
            %li
              %a.close-link
                %i.fa.fa-close
          .clearfix
        .x_content
          .dashboard-widget-content
            %ul.quick-list
              %li
                %i.fa.fa-line-chart
                %a{:href => "#"} Achievements
              %li
                %i.fa.fa-thumbs-up
                %a{:href => "#"} Favorites
              %li
                %i.fa.fa-calendar-o
                %a{:href => "#"} Activities
              %li
                %i.fa.fa-cog
                %a{:href => "#"} Settings
              %li
                %i.fa.fa-area-chart
                %a{:href => "#"} Logout
            .sidebar-widget
              %h4 Profile Completion
              %canvas#foo{:height => "80", :style => "width: 160px; height: 100px;", :width => "150"}
              .goal-wrapper
                %span#gauge-text.gauge-value.pull-left 0
                %span.gauge-value.pull-left %
                %span#goal-text.goal-value.pull-right 100%

    / start of weather widget
    .col-md-4.col-sm-6.col-xs-12
      .x_panel
        .x_title
          %h2
            Today's Weather
            %small Sessions
          %ul.nav.navbar-right.panel_toolbox
            %li
              %a.collapse-link
                %i.fa.fa-chevron-up
            %li
              %a.close-link
                %i.fa.fa-close
          .clearfix
        .x_content
          .row
            .col-sm-12
              .temperature
                %b> Monday
                , 07:30 AM
                %span F
                %span
                  %b C
          .row
            .col-sm-4
              .weather-icon
                %span
                  %canvas#partly-cloudy-day{:height => "84", :width => "84"}
            .col-sm-8
              .weather-text
                %h2
                  Texas
                  %br/
                  %i Partly Cloudy Day
          .col-sm-12
            .weather-text.pull-right
              %h3.degrees 23
          .clearfix
          .row.weather-days
            .col-sm-2
              .daily-weather
                %h2.day Mon
                %h3.degrees 25
                %span
                  %canvas#clear-day{:height => "32", :width => "32"}
                %h5
                  15
                  %i km/h
            .col-sm-2
              .daily-weather
                %h2.day Tue
                %h3.degrees 25
                %canvas#rain{:height => "32", :width => "32"}
                %h5
                  12
                  %i km/h
            .col-sm-2
              .daily-weather
                %h2.day Wed
                %h3.degrees 27
                %canvas#snow{:height => "32", :width => "32"}
                %h5
                  14
                  %i km/h
            .col-sm-2
              .daily-weather
                %h2.day Thu
                %h3.degrees 28
                %canvas#sleet{:height => "32", :width => "32"}
                %h5
                  15
                  %i km/h
            .col-sm-2
              .daily-weather
                %h2.day Fri
                %h3.degrees 28
                %canvas#wind{:height => "32", :width => "32"}
                %h5
                  11
                  %i km/h
            .col-sm-2
              .daily-weather
                %h2.day Sat
                %h3.degrees 26
                %canvas#cloudy{:height => "32", :width => "32"}
                %h5
                  10
                  %i km/h
            .clearfix
    / end of weather widget
    .col-md-4.col-sm-6.col-xs-12
      .x_panel.fixed_height_320
        .x_title
          %h2
            Incomes
            %small Sessions
          %ul.nav.navbar-right.panel_toolbox
            %li
              %a.collapse-link
                %i.fa.fa-chevron-up
            %li
              %a.close-link
                %i.fa.fa-close
          .clearfix
        .x_content
          .dashboard-widget-content
            %ul.quick-list
              %li
                %i.fa.fa-bars
                %a{:href => "#"} Subscription
              %li
                %i.fa.fa-bar-chart
                %a{:href => "#"} Auto Renewal
              %li
                %i.fa.fa-support
                %a{:href => "#"} Help Desk
              %li
                %i.fa.fa-heart
                %a{:href => "#"} Donations
            .sidebar-widget
              %h4 Goal
              %canvas#foo2{:height => "80", :style => "width: 160px; height: 100px;", :width => "150"}
              .goal-wrapper
                %span.gauge-value.pull-left $
                %span#gauge-text2.gauge-value.pull-left 3,200
                %span#goal-text2.goal-value.pull-right $5,000